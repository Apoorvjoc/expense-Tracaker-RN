vector<int> createTargetArray(vector<int>& nums, vector<int>& index) {
        vector<int>target(nums.size() , INT_MAX);
        for(int i = 0 ; i<nums.size() ; i++){
            if(target[index[i]] == INT_MAX){
                target[index[i]] = nums[i];
            }else{
                for(int j = i ; j>index[i] ; j--){
                    target[j] = target[j-1];
                }
                target[index[i]] = nums[i];
            }
        }
        return target;
    }
    