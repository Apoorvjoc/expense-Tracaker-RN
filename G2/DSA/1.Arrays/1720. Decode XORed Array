 vector<int> decode(vector<int>& encoded, int first) {
        vector<int>ans;
         ans.push_back(first);
        int sec = first;
        for(int i = 0 ; i<encoded.size();i++){
            int elem = encoded[i]^first;
            ans.push_back(elem);
            first = elem;
        }
        return ans;
    }